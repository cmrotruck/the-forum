<div class="ui two stackable cards">
  {{#each goup as |group|}}
    <div class="card">
      <div class="image">
        <img src={{ group_image }} />
      </div>
      <div class="content">
        <a class="header" href="/group/{{ id }}">{{ group_name }}</a>
      </div>
    </div>
  {{/each}}
</div>